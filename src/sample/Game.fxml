<AnchorPane fx:id="Game" prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #123456;" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Game">
   <children>
      <GridPane fx:id="grid" alignment="CENTER" gridLinesVisible="true" layoutX="188.0" layoutY="30.0" prefHeight="540.0" prefWidth="800.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="10.0">
      </GridPane>
      <Label fx:id="winLabel" alignment="CENTER" layoutX="306.0" layoutY="265.0" prefHeight="71.0" prefWidth="188.0" text="YOU WIN" textFill="WHITE">
         <font>
            <Font name="System Bold" size="42.0" />
         </font>
      </Label>
      <Button fx:id="back" focusTraversable="false" mnemonicParsing="false" onAction="#backClicked" prefHeight="60.0" prefWidth="60.0">
         <graphic>
            <ImageView fitHeight="29.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@/Images/back.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</AnchorPane>
